myFruits=["banana", "apple", "cherry"]
print(myFruits)
print(type(myFruits))
print(myFruits[0])
print(myFruits[1])
print(myFruits[2])
myFruits[2] = "orange"
print(myFruits)


myFinAnsTul = ("apple", "banana", "pineapple")
print(myFinAnsTul)
print(type(myFinAnsTul))
print(myFinAnsTul[0])
print(myFinAnsTul[1])
print(myFinAnsTul[2])

myFavFruitDic = {
                 "Adam" : "apple",
                 "Ben" : "banana", 
                 "Penny" : "pineapple"
                }
print(myFavFruitDic)
print(type(myFavFruitDic))
print(myFavFruitDic["Adam"])
print(myFavFruitDic["Ben"])
print(myFavFruitDic["Penny"])
